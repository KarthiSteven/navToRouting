<mvc:View
xmlns:core="sap.ui.core"
xmlns:mvc="sap.ui.core.mvc"
xmlns="sap.m"
controllerName="tableContainer.tablecontainer.controller.Detail">
<Page
id="detailPage"
title="{products>ProductName}"
navButtonPress="onNavPress"
showNavButton="true">
<content>
<ObjectHeader
id="objectHeader"
intro="{products>ProductName}" 
title="{products>QuantityPerUnit}">
</ObjectHeader>
</content>
</Page>
</mvc:View>